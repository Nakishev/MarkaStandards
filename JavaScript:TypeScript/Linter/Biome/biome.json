{
  "linter": {
    "rules": {
      "suspicious": {
        "noExplicitAny": "off",
        "noArrayIndexKey": "off"
      },
      "a11y": {
        "useKeyWithClickEvents": "off",
        "noNoninteractiveTabindex": "off"
      },
      "correctness": {
        "useExhaustiveDependencies": "off"
      },
      "complexity": {
        "noForEach": "off"
      }
    }
  }
}
